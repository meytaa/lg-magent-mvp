# Copy to `.env` and fill in your keys. Do NOT commit real keys.

# OpenAI
OPENAI_API_KEY="<your-openai-api-key>"

# LangSmith / LangChain tracing
# Either LANGCHAIN_* (preferred) or LANGSMITH_* variables work.
LANGSMITH_TRACING="true"
LANGSMITH_API_KEY="<your-langsmith-api-key>"
LANGSMITH_PROJECT="lg-magent-mvp"
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"

# Optional: direct LangChain vars if you prefer
# LANGCHAIN_TRACING_V2="true"
# LANGCHAIN_API_KEY="<your-langsmith-api-key>"
# LANGCHAIN_PROJECT="lg-magent-mvp"
# LANGCHAIN_ENDPOINT="https://api.smith.langchain.com"

# Model overrides (optional)
# ROUTER_MODEL="gpt-4o-mini"
# FINALIZE_MODEL="gpt-4o"
# VISION_MODEL="gpt-4o"
# EMBED_MODEL="text-embedding-3-small"

# Output preferences
# OUTPUT_FORMAT="json+narrative"
# APPROVALS="auto"  # or "pause-before-finalize"
